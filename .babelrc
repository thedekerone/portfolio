{
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        },
      ],
      ["transform-class-properties"],
      ["@babel/plugin-syntax-dynamic-import"]
    ],
    "presets":[
        "@babel/preset-env",
        "@babel/preset-react"
    ]
  }